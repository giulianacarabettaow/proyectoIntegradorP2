<!DOCTYPE html>
<html lang="es">
	<%- include('./partials/head') %>

<body>
	<%- include('./partials/headerLogueado', {users: users}) %>
	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>

			<form method="GET" action="/products">
						
						
						<label class="form-label">Nombre</label>
						<input type="text" name="nombre" placeholder="Sillon X" class="form-input">
					
						
						<label class="form-label">Descripción</label>
						<input type="text" name="descripcion" class="form-input">
						

						
						<label class="form-label">Precio</label>
						<input type="text" name="precio" class="form-input">
						

						
						<label class="form-label">Imagén</label>
						<input type="text" name="imagen" class="form-input">
					
						
						<button type="submit" class="form-input">Agregar producto</button>
					</form>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> </h2>


	<%- include('./partials/footer') %>
</body>

</html>